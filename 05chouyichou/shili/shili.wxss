/* 05chouyichou/shili/shili.wxss */
page{
    padding: 50rpx 0 80rpx;
    box-sizing: border-box;
}
.example-box{
    text-align: center;
    margin-bottom: 48rpx;
}
.example-box .pic{
    width: 212rpx;
    height: 320rpx;
    margin: 0 auto 40rpx;
}
.example-box .pic image{
    width: 100%;
    display: block;
    border-radius: 20rpx;
}
.example-box .txt{
    font-size: 28rpx;
    color: #FFB58C;
    line-height: 38rpx;
}
.example-form .ipt-info{
    height: 104rpx;
	background: rgba(29, 31, 48, 0.8);
	border: 0.5px solid rgba(255, 255, 255, 0.2);
	backdrop-filter: blur(12.2323px);
	border-radius: 24rpx;
	display: block;
	margin-bottom: 24rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
}
.example-form .textarea-box{
	position: relative;
	margin-bottom: 118rpx;
}
.example-form .textarea-box textarea{
	display: block;
	width: 100%;
	height: 320rpx;
	background: rgba(29, 31, 48, 0.8);
	border: 0.5px solid rgba(255, 255, 255, 0.2);
	backdrop-filter: blur(12.2323px);
	border-radius: 24rpx;
  padding: 24rpx;
  box-sizing: border-box;
}
.example-form .textarea-box .btn-yuyin{
	position: absolute;
	bottom: 24rpx;
	left: 24rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	width: 208rpx;
	height: 72rpx;
	line-height: 70rpx;
	background: linear-gradient(248.27deg, rgba(119, 138, 255, 0.096) 18.05%, rgba(255, 255, 255, 0.128) 83.27%);
	border: 0.5px solid rgba(255, 255, 255, 0.2);
	border-radius: 16rpx;
	color: #B6B2CF;
}
.example-form .textarea-box .btn-yuyin image{
	width: 32rpx;
	height: 32rpx;
	margin-right: 16rpx;
}
.example-form .btn-submit{
	display: block;
	font-size: 32rpx;
	color: #ffffff;
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	background: #7461E7;
	border-radius: 72rpx;
}
.example-form radio-group{
	margin-bottom: 48rpx;
}
.label-radio{
	position: relative;
	padding-left: 56rpx;box-sizing: border-box;
	background: url(data:image/png;base64,) no-repeat left center;
	-webkit-background-size: 48rpx auto;background-size: 48rpx auto;
	margin-right: 32rpx;
}
.label-radio[aria-checked="true"]{
	background-image: url(data:image/png;base64,);
}
.label-radio radio{
	position: absolute;
	left: -9999px;
}
.pop-win2 .desc{
    margin-top: 130rpx;
}
.pop-img{
    width: 212rpx;
    height: 320rpx;
    margin: 48rpx auto 24rpx;
}
.pop-img image{
    display: block;
    width: 100%;
}
.pop-win2 .txt{
    text-align: center;
    color: #FFB58C;
    margin-bottom: 48rpx;
}
.pop-win2 .p1{
    padding: 0 48rpx;
    box-sizing: border-box;
    text-align: center;
}


























