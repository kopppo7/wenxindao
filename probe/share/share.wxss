page{
  padding: 40rpx 0 64rpx;
  box-sizing: border-box;
}
.canvas-box{
	padding: 30rpx 56rpx;
	box-sizing: border-box;
	width: 670rpx;
	/*height: 1230rpx;*/
	margin: 0 auto;
	position: relative;
	z-index: 1;
	background-color: #fff;
	border-radius: 50rpx;
	overflow: hidden;
}
.canvas-bg{
	position: absolute;width: 100%;top: 0;left: 0;
	z-index: 1;
}
.canvas-content{
	position: relative;
	z-index: 2;
	height: 100%;
	padding-bottom: 160rpx;
}
.canvas-box .s1{
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-bottom: 22rpx;
}
.canvas-box .s1-l{
	width: 64rpx;
  height: 64rpx;
  border-radius:54rpx;
}
.canvas-box .s1-r{
	display: flex;
	align-items: center;
	color: #2a2230;
}
.canvas-box .s1-r .days{
	font-size: 112rpx;
}
.canvas-box .s1-r .date-box .lc{
	position: relative;
	font-size: 32rpx;
	margin-bottom: 20rpx;
}
.canvas-box .s1-r .date-box .lc:after{

}
.canvas-box .s1-r .date-box .date{
	font-size: 40rpx;
}
.canvas-box .s2{
	display: block;
	width: 100%;
	height: 592rpx;
	margin-bottom: 10rpx;
}
.canvas-box .s3{
	position: relative;
	display: flex;
	justify-content: center;
	align-items: center;
	margin-bottom: 60rpx;
	height: 60rpx;
}
