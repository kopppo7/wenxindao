.canvas-box .s4{
	font-size: 16rpx;
	text-align: center;
	color: #000000;
	opacity: .7;
  padding: 0 60rpx;
  box-sizing: border-box;
	margin-bottom: 40rpx;
}
.canvas-box .s5{
	display: flex;
	justify-content: space-between;
	align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.canvas-box .s5-l{
	display: flex;
}
.canvas-box .s5-l-item{

}
.canvas-box .s5-l-item + .s5-l-item{
	margin-left: 100rpx;
}
.canvas-box .s5-l-item .s5-l-tit{
	color: #000000;
}
.canvas-box .s5-l-item .s5-l-time{
	font-size: 40rpx;
	color: #d7744a;
}
.canvas-box .s5-r{
	background: #FFFFFF;
	width: 160rpx;
	height: 160rpx;
	border: 1px solid rgba(0, 0, 0, 0.1);
	border-radius: 16rpx;
  padding: 12rpx;
  box-sizing: border-box;
}
.canvas-box .s5-r .qr{
	display: block;
	width: 100%;
	height: 100%;
}
.tools-box{
	display: flex;
	justify-content: center;
	margin-top: 36rpx;
}
.tools-box .tools-item{
	display: flex;
	justify-content: center;
	align-items: center;
	width: 208rpx;
	height: 96rpx;
	background: #ffffff;
	border-radius: 48rpx;
	color: #2a2230;
	margin: 0 12rpx;
}
.tools-box .tools-item image{
	width: 32rpx;
	height: 32rpx;
	margin-right: 8rpx;
}
.myCanvas{
	margin: 0 auto;
	width: 670rpx;
	/*height: 100vh;*/
	position: fixed;left: -99999px;top: 0;  background-color: transparent;
	opacity: 0;
	z-index: -2;
}

.date-mod{
	position: absolute;
	right: -20rpx;
	top: 10rpx;
	display: flex;
	align-items: center;
	line-height: 1;
	width: 300rpx;
}
.date-mod .days{
	font-size: 112rpx;
	line-height: 1;
	min-width: 150rpx;
}
.date-mod .date-box{
	margin-left: 8rpx;
	font-size: 0;
	min-width: 150rpx;
}
.date-mod .date-box .lc{
	font-size: 30rpx;
	line-height: 1;
}
.date-mod .date-box .gang{
	line-height: 1;
	margin: 8rpx 20rpx;
	display: inline-block;
	-webkit-transform-origin: 50% 50%;-moz-transform-origin: 50% 50%;-ms-transform-origin: 50% 50%;-o-transform-origin: 50% 50%;transform-origin: 50% 50%;
	-webkit-transform: rotate(30deg);-moz-transform: rotate(30deg);-ms-transform: rotate(30deg);-o-transform: rotate(30deg);transform: rotate(30deg);
}
.date-mod .date-box .date{
	font-size: 36rpx;
	line-height: 1;
}
.head-img{
	position: absolute;
	display: flex;
	align-items: center;
	left: 50%;
	-webkit-transform: translateX(-50%);-moz-transform: translateX(-50%);-ms-transform: translateX(-50%);-o-transform: translateX(-50%);transform: translateX(-50%);
	top: 156rpx;
	width: 330rpx;
}
.head-img .name{
	flex:1;
	width: 0;
	white-space: nowrap;overflow: hidden;text-overflow: ellipsis;
	font-size: 28rpx;
	color: #fff;
	margin-left: 20rpx;
}
.head-img .tuijian{
	width: 120rpx;
	height: 40rpx;
	border-radius: 24rpx;
	background-color: #EE9848;color: #ffffff;
	font-size: 24rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}
.content{
	padding-top: 250rpx;
	width: 100%;
	overflow: hidden;
}
.text-mod-top{
	display: flex;
	justify-content: space-between;
}
.text-mod-top .l{
	flex:0 0 200rpx
}
.text-mod-top .r{
	flex:0 0 350rpx;
}
.content .card{
	width: 350rpx;
	height: 526rpx;
	overflow: hidden;
	border-radius: 24rpx;
	border: 4px solid rgba(182, 178, 207, 0.18);
}
.content .title{
	color: #000000;
	font-size: 36rpx;
	text-align: center;
	margin-bottom: 30rpx;

}
.content .text{
	font-size: 28rpx;
	color: #222537;
	text-indent: 2em;
}
.content .text.text2{
	float: none;
	width: auto;
	text-indent: 0;
}

.content .yin{
	width: 40rpx;
	height: 40rpx;
	text-align: left;
}
.content .yin.end{
	display: inline-block;
}
.content .card image{
	display: block;width: 100%;height: 100%;
}
.canvas-content .s5{
	position: absolute;
	bottom: 5rpx;
	right: -17rpx;
	width: 100%;
	padding-right: 0;
}
.s5-l .gang{
	color: #EE9848;
	font-size: 60rpx;
	margin: 0 20rpx;
}

.ts-det{
	display: flex;
	align-items: center;
	padding-top: 280rpx;
}
.ts-det .pic{
	flex:0 0 120rpx;
	height: 160rpx;
	box-shadow: inset 0px -1px 1px rgba(0, 0, 0, 0.3), inset 0px 1px 0.5px rgba(255, 255, 255, 0.25);
	margin-right: 24rpx;
}
.ts-det .pic image{
	display: block;width: 100%;height: 100%;
}
.ts-det .txt{
	flex-grow: 1;
	width: calc(100% - 135rpx);
}
.ts-det .txt .title{
	white-space: nowrap;overflow: hidden;text-overflow: ellipsis;
	font-size: 28rpx;
	color: #222537;
	line-height: 60rpx;
	height: 60rpx;
}
.ts-det .txt .desc{
	overflow: hidden;display: -webkit-box;-webkit-line-clamp: 3;-webkit-box-orient: vertical;
	font-size: 26rpx;
	color: #222537;
	opacity: .7;
	text-align: justify;
}
.eval-list{
	margin-top: 70rpx;
}
.eval-list .title{
	color: #000000;
	font-size: 32rpx;
	margin-bottom: 20rpx;
}
.eval-item .text{
	font-size: 28rpx;
	color: #222537;
	line-height: 44rpx;
	overflow: hidden;
	word-break: break-all;
}
.eval-item+.eval-item{
	margin-top: 30rpx;
}
.eval-item image{
	width: 40rpx;
	height: 40rpx;
	line-height: 40rpx;
}
.eval-item{
	padding-left: 50rpx;
	padding-right: 50rpx;
	position: relative;
}
.eval-item .start{
	position: absolute;
	left: 0;
	top: 0;
}
.eval-item .end{
	position: absolute;
	right: 10rpx;
	bottom: 0;
}
.bot-info{
	display: flex;
	justify-content: space-between;
	align-items: center;
	position: absolute;
	bottom: 60rpx;
	left: 0;
	width: 100%;
}
.bot-info .left{
	color: #ee9848;
	font-weight: bold;
	font-size: 32rpx;
}
.bot-info .left .text{
	white-space: nowrap;
}
.bot-info .left image{
	width: 108rpx;
	height: 24rpx;
}
.bot-info .right{
	width: 160rpx;
	height: 160rpx;
	border-radius: 12rpx;
	background-color: #fff;
	padding: 20rpx;
}
.bot-info .right image{
	display: block;width: 100%;height: 100%;
}

.canvas-content{
	padding-bottom: 240rpx;
}
