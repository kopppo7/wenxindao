/* 04zhutipaidui/setHouse/setHouse.wxss */
.setHouse{padding: 48rpx 40rpx;box-sizing: border-box;}
.setHouse .top{margin-bottom: 96rpx;}
.setHouse .top .pic{display: block;width: 224rpx;height: 224rpx;border-radius: 8px 8px 8px 8px;margin: 0 auto 48rpx;}
.setHouse .top .txt{font-size: 28rpx;color: rgba(255,255,255,.7);padding: 0 16rpx;text-align: center;box-sizing: border-box;}
.setHouse .bot{}
.setHouse .bot .qx{
    border: 1px solid rgba(255,255,255,0.2);
    background: rgba(182,178,207,0.18);
    border-radius: 12px 12px 12px 12px;
    margin-top: 60rpx;
    padding: 24rpx 24rpx 44rpx;
    box-sizing: border-box;
    margin-bottom: 100rpx;
}
.setHouse .bot .qx .img{text-align: center;margin-bottom: 20rpx;margin-top: -60rpx;}
.setHouse .bot .qx .img image{width: 112rpx;height: 112rpx;border-radius: 112rpx;display: block;margin: 0 auto;}
.setHouse .bot .qx .img text{
    width: 120rpx;
    height: 48rpx;
    background: #FFB58C;
    border-radius: 24rpx;
    font-size: 22rpx;color: #010206;
    display: inline-block;
    position: relative;
    top: -20rpx;
    line-height: 48rpx;
}
.setHouse .bot .sf{display: -webkit-flex;display: flex;justify-content: space-between;/* 水平居中 */align-items:center;margin-bottom: 20rpx;}
.setHouse .bot .inp input{
    line-height: 92rpx;
    background: rgba(29,31,48,0.8);
    border-radius: 24rpx;
    height: 92rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
    opacity: 1;
    border: 1px solid rgba(255,255,255,0.2);
}
