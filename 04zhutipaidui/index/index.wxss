/* 03shenmingtansuo/index/index.wxss */

.z-list1 {
	padding: 0;
	background: transparent;
}
.z-list1 .target{
    position: absolute;
    top: 0;
    right: 0;
}
.z-list1 .target view{
  padding: 10rpx 16rpx;
  font-size: 18rpx;
  background-color: rgba(137, 235, 241, 0.3);
  border-top-right-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
  color: #89EBF1;
}
.z-list1 .item {
  position: relative;
	display: flex;
	align-items: center;
	background: rgba(182, 178, 207, 0.12);
	backdrop-filter: blur(12px);
	border-radius: 16rpx;
	margin-bottom: 24rpx;
	height: 240rpx;
  padding: 24rpx;
  box-sizing: border-box;
}

.z-list1 image.pic {
	flex-shrink: 0;
	height: 192rpx;
	margin-right: 28rpx;
	object-fit: cover;
	width: 132rpx;
}

.z-list1 .txt {
	flex: 1;
}

.z-list1 .txt .s1 {
	display: flex;
	align-items: center;
	margin-bottom: 16rpx;
}

.z-list1 .txt .s1 .title {
	font-size: 32rpx;
}

.z-list1 .txt .s1 .tags {
	font-size: 22rpx;
	width: 68rpx;
	height: 36rpx;
	line-height: 36rpx;
	background: linear-gradient(77.56deg, #25C28A -4.34%, #40CBE4 100.39%);
	border-radius: 16rpx 0 16rpx 0;
	color: rgba(0, 0, 0, 0.9);
	text-align: center;
	margin-left: 20rpx;
}

.z-list1 .txt .s1 .tags.hots-tag {
	background: linear-gradient(77.56deg, #EA5D70 -4.34%, #FF9572 101.49%, #FF9572 101.49%);
}

.z-list1 .txt .s2 {
	display: flex;
	font-size: 24rpx;
	margin-bottom: 48rpx;
}

.z-list1 .txt .s2 text {
	color: #b6b2cf;
}

.z-list1 .txt .s2 text+text {
	position: relative;
	margin-left: 34rpx;
}

.z-list1 .txt .s2 text+text:before {
	content: '';
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-50%);
	-moz-transform: translateY(-50%);
	-ms-transform: translateY(-50%);
	-o-transform: translateY(-50%);
	transform: translateY(-50%);
	left: -18rpx;
	width: 1px;
	height: 18rpx;
	background: #B6B2CF;
}

.z-list1 .txt .s3 {
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.z-list1 .txt .s3 .s3-l {
	font-size: 24rpx;
	color: #b6b2cf;
}

.z-list1 .txt .s3 .s3-l image {
	width: 20rpx;
	height: 20rpx;
	margin-right: 2px;
}

.z-list1 .txt .s3 .s3-r1 {
	font-size: 24rpx;
	width: 152rpx;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
	background: #7461E7;
	border: 0.5px solid rgba(255, 255, 255, 0.2);
	backdrop-filter: blur(12px);
	border-radius: 46rpx;
}

.z-list1 .txt .s3 .s3-r2 {
	position: relative;
	font-size: 24rpx;
	color: #ffffff;
	width: 150rpx;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
	background: linear-gradient(90deg, #7461E6 0%, #7461E6 21.23%, rgba(255, 255, 255, 0) 21.7%), linear-gradient(260.43deg, rgba(119, 138, 255, 0.24) 2.12%, rgba(255, 255, 255, 0.32) 102.79%);
	border: 0.5px solid rgba(255, 255, 255, 0.2);
	backdrop-filter: blur(12px);
	border-radius: 46rpx;
}

.z-list1 .txt .s3 .s3-r3 {
	font-size: 24rpx;
	color: #773808;
	width: 204rpx;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
	background: url(data:image/png;base64,) no-repeat center / cover;
}
.nodata{
	text-align: center;font-size: 32rpx;color: #fff;margin: 40rpx 0;
}